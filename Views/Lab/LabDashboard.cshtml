@model IEnumerable<intex.Models.LabDashboardInfo>

@{
    ViewBag.Title = "Lab Dashboard";
}

<h2>Lab Dashboard</h2>
<br />

<table class="table">
    
        <tr class="active">
            <th>
                @Html.DisplayNameFor(model => model.LTNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompoundName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkOrderID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OrderDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StatusDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompleteDate)
            </th>
        </tr>
    
@foreach (var item in Model)
{
    <tr>
        <td>
            <a href="@Url.Action("LabCompound", new { LT = item.LTNumber})">
             @Html.DisplayFor(modelItem => item.LTNumber)
            </a>
            @*@Html.ActionLink(model.item.LTNumber, "LabCompound", new { LT = item.LTNumber })*@
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CompoundName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WorkOrderID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrderDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StatusDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CompleteDate)
        </td>
    </tr>
}
</table>

<p>
    @Html.ActionLink("Back", "Index", null, new { @class = "btn btn-primary" })
</p>
